// {{pascalCase name}} API functions

const API_BASE = import.meta.env.VITE_API_URL

export async function get{{pascalCase name}}(): Promise<any> {
  const response = await fetch(`${API_BASE}/api/{{dashCase name}}`, {
    headers: {
      'Authorization': `Bearer ${localStorage.getItem('token')}`,
    },
  })

  if (!response.ok) {
    throw new Error('Failed to fetch {{dashCase name}}')
  }

  return response.json()
}